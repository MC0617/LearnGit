#########
$git init
	//初始化一个git仓库
$git add <file>
	//添加文件，可多次使用以添加多个文件
$git commit [-m "help,note.."]
	//提交更改
$git status
	//查看工作区状态
$git diff
	//查看所做更改
$git reset --hard commit_id
	//回退提交的更改
	//回退到commit_id对应版本
	//HEAD 对应当前版本，上一版本为HEAD^，上上版本为HEAD^^
	//上100版本：HEAD~100
$git log
	//查看提交历史
	//加上" --pretty=oneline"只参数显示版本号和提交记录
$git reflog
	//查看命令历史，确定未来版本。。
$git reset HEAD file
	//回退暂存区的添加
$git chechout -- file
	//丢弃工作区文件的修改
	//用版本库里的版本替换工作区的版本
$git rm
	//删除版本库文件，需要commit
$ssh-keygen -t rsa -C "email@example.com"
	//创建SSH Key
	//GitHub，Account settings，SSH Key，Add SSH Key，拷贝id_rsa.pub
$git remote add origin https://github.com/MC0617/LearnGit.git
	//添加、关联远程库，origin可改
$git push [-u] origin master
	//将本地库内容推送到远程
	//首次推送需要[-u]参数

